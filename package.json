    "@types/amqplib": "^0.10.7",
    "@types/express": "^4.17.21",
    "@types/node": "^20.14.2",
    "prisma": "^5.15.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.4.5"
  }
